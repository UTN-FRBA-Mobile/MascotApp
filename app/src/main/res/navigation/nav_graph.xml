    <fragment
        android:id="@+id/mascotInfoFragment"
        android:name="com.utn.MascotApp.fragments.MascotInfoFragment"
        android:label="fragment_mascot_info"
        tools:layout="@layout/fragment_mascot_info" >
        <action
            android:id="@+id/action_mascotInfoFragment_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment"
            app:enterAnim="@anim/to_right"
            app:exitAnim="@anim/to_right"
            app:popUpTo="@id/mainMenuFragment" />
        <!--        <argument-->
        <!--            android:name="image"-->
        <!--            app:argType="string"-->
        <!--            app:nullable="true" />-->
        <argument
            android:name="image"
            app:argType="string" />
        <argument
            android:name="name"
            app:argType="string" />
        <argument
            android:name="description"
            app:argType="string" />
        <argument
            android:name="age"
            app:argType="integer" />
        <argument
            android:name="sex"
            app:argType="string" />
        <argument
            android:name="color"
            app:argType="string" />
        <argument
            android:name="breed"
            app:argType="string" />
        <argument
