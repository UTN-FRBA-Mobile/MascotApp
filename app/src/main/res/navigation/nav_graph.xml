        android:name="com.utn.MascotApp.fragments.PublicarFragment"
        android:label="PublicarFragment" >
        <action
            android:id="@+id/action_publicarFragment2_to_mainMenuFragment"
            app:destination="@id/mainMenuFragment" />
        <action
